<!DOCTYPE html>
<html>
  <head>
    <title>Foco</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <% if current_user %>
      Hi, <%= current_user.name %>
      <%= link_to 'Log out', logout_path %>
    <% else %>
      <%= link_to 'Sign up', new_user_path %>
    <% end %>

    <%= yield %>

  </body>
</html>
