<div class="project_list_wrapper">
    <%= link_to 'Add project', new_project_path, class: "add_project_button" %>

    <div class="project_list_search">
        <form action="">
            <input name="" type="text" onfocus="if(this.value=='Search')this.value=''" onblur="if(this.value=='')this.value='Search'" value="Search">
            <a href="#" onclick="$(this).parent().submit(); return false;">Search</a>
        </form>
    </div>
    <nav class="project_list_nav">
        <a href="#">All projects</a>
        <a href="#">My projects</a>
        <a href="#">To review</a>
    </nav>
    <ul class="project_list">
    <% @projects.each do |project| %>
      <li>
        <% if project.user == current_user %>
            <%= link_to 'Detail', project %>
        <% else %>
            <%= link_to 'Add review', review_project_path(project) %>
        <% end %>
        <div class="project_list_interest">test 98%</div>
        <h4><%= project.name %></h4>
        <p><%= project.description %></p>
      </li>
    <% end %>
    </ul>
</div>
